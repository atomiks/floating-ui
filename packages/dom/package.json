{
  "name": "@floating-ui/dom",
  "version": "0.0.1",
  "description": "Floating UI for the web",
  "main": "dist/floating-ui.dom.js",
  "module": "dist/floating-ui.dom.mjs",
  "unpkg": "dist/floating-ui.dom.min.js",
  "sideEffects": false,
  "files": [
    "dist/",
    "index.d.ts",
    "src/**/*.d.ts"
  ],
  "browserslist": "> 1%, not dead",
  "scripts": {
    "dev": "concurrently 'rollup -c -w' 'serve -l 1234 test/visual'",
    "build": "NODE_ENV=build rollup -c",
    "test": "jest test",
    "test:functional": "rollup -c && npx playwright test",
    "test:functional:update": "rollup -c && npx playwright test -u"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@floating-ui/core": "file:../core"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@playwright/test": "^1.16.3",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/jest": "^27.0.3",
    "concurrently": "^6.4.0",
    "jest": "^27.3.1",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^13.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  }
}
